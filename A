#include <stdio.h>
#include <stdlib.h>
#include <string.h>
int main(int argc, char **argv) {
  unsigned long n = strtoul(argv[1],NULL,10), i = 0, j, c, sign;
  char s[] = "";
  if((sign = n) < 0)
    n = -n;
  do {
    s[i++] = (char)n % 10 + '0';
  } while((n /= 10) > 0);
  if(sign < 0)
    s[i++] = '-';
  s[i] = '\0';
  j = strlen(s);
  for(i = 0, j = strlen(s)-1; i < j; i++, j--) {
    c = s[i];
    s[i] = s[j];
    s[j] = (char)c;
  }
  puts(s);
  return 0;
}
